{
  "expo": {
    "name": "TaskAssist.exe",
    "description": "Windows95 style react-native todo-list app. See https://github.com/noahhorlacher/taskassist-exe",
    "slug": "taskassistexe",
    "version": "1.0.0",
    "orientation": "portrait",
    "backgroundColor": "#008080",
    "primaryColor": "#008080",
    "icon": "./assets/icon.png",
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#008080"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.noahhorlacher.taskassist",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.noahhorlacher.taskassist",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#008080"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
